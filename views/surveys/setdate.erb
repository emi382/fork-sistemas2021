<body>
  <section>
    <form action="/surveys/careercount" method="get">
        <label for="start">Start date:</label>
        <input type="date" id="start" name="startDate"
            value=<%=first_date%> 
        >
        <label for="finish">Finish date:</label>
        <input type="date" id="start" name="finishDate"
            value=<%=last_date%> 
        >
        <label for="qcareer">Career id:</label>
              <select class="text-l" name="career">
                <%careers.map do |careers|%>
                <option class="text-l" value = <%=careers.career_id%> > <%=careers.name%> </option>
                <%end%>
              </select>
        <button type="submit" name="button">Buscar</button>
    </form>
  </section>
